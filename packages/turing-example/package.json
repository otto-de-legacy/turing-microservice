{
  "name": "turing-example",
  "version": "0.45.1",
  "description": "An example of how the turing-microservice can be used.",
  "main": "src/server/App.js",
  "scripts": {
    "build": "rm -rf resources/server/public && NODE_ENV=production webpack --progress --config ./resources/client/webpack/webpack.config.prod.js",
    "test": "stylelint '{**/,}*.{css,scss}' && eslint '{**/,}*.{js,jsx}' && tslint -p ./tsconfig.json --type-check 'src/client/{**/,}*.{ts,tsx}' && NODE_ENV=test ACTIVE_PROFILE=test TURING_CONFIG_DIR=./resources/server/config jest",
    "test:e2e": "BASE_DOMAIN=http://localhost:8080/ protractor ./test-resources/e2e/protractor.conf.js",
    "webdriver:install": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "start": "NODE_ENV=production ACTIVE_PROFILE=production TURING_CONFIG_DIR=./resources/server/config node bin/Server.js",
    "watch": "nodemon bin/Server.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "jest-tsconfig.json"
      }
    },
    "collectCoverageFrom": [
      "<rootDir>/src/{client,server}/{**/,}*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/whatwg-fetch/fetch.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "<rootDir>/src/{client,server}/{**/,}*.test.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy",
      "^(?!.*\\.(css|json)$)": "<rootDir>/__mocks__/fileMock.js"
    },
    "modulePathIgnorePatterns": [
      "(.idea|target|coverage|node_modules)"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    },
    "plugins": [
      [
        "react-css-modules",
        {
          "generateScopedName": "[name]__[local]___[hash:base64:5]"
        }
      ]
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 version",
              "ie 9"
            ]
          },
          "modules": false,
          "useBuiltIns": true
        }
      ],
      "react"
    ]
  },
  "homepage": "https://github.com/otto-de/turing-microservice/tree/master/packages/turing-example#readme",
  "bugs": {
    "url": "https://github.com/otto-de/turing-microservice/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:otto-de/turing-microservice.git"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cookie-parser": "1.4.3",
    "express": "^4.15.3",
    "http-status": "1.0.1",
    "turing-config": "^0.39.1",
    "turing-health": "^0.45.1",
    "turing-logging": "^0.44.1",
    "turing-mongo": "^0.45.1",
    "turing-server": "^0.45.1",
    "turing-status": "^0.45.1",
    "turing-vault": "^0.44.2",
    "winston-graylog2": "0.6.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.1",
    "@types/jest": "^20.0.2",
    "@types/material-ui": "^0.18.0",
    "@types/react": "^16.0.2",
    "@types/react-css-modules": "^4.2.1",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.3",
    "@types/react-redux": "^5.0.1",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.2.1",
    "babel-plugin-react-css-modules": "^3.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "extract-text-webpack-plugin": "^3.0.0-beta.3",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "jasmine-spec-reporter": "^4.1.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.4.0",
    "lodash._baseuniq": "^4.6.0",
    "material-ui": "^0.19.0",
    "nodemon": "^1.11.0",
    "offline-plugin": "^4.8.1",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "protractor": "^5.1.1",
    "react": "^15.6.1",
    "react-dev-utils": "^4.0.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "supertest": "3.0.0",
    "ts-jest": "^20.0.6",
    "tslint": "^5.4.3",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-language-service": "^0.9.6",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "author": {
    "name": "Benedikt Stemmildt",
    "email": "benedikt.stemmildt@otto.de",
    "url": "https://github.com/benestem"
  },
  "license": "Apache-2.0",
  "bin": {
    "turing-example": "bin/Server.js"
  }
}
